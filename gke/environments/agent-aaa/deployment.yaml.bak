apiVersion: apps/v1beta2
kind: Deployment
metadata:
  name: proxy-agent-aaa
  labels:
    app: proxy-agent-aaa
spec:
  selector:
    matchLabels:
      app: proxy-agent-aaa
  template:
    metadata:
      labels:
        app: proxy-agent-aaa
    spec:
      containers:
      - name: proxy-agent
        image: gcr.io/mam-nooage/agent:sed
        imagePullPolicy: Always
        env:
          # Must be the same as in agent/service.yaml
          - name: JUPYTERLAB_SERVICE_NAME
            value: jupyterlab-aaa
          # Used to create the unique configmap when deploying the agent.
          - name: JUPYTERLAB_ID
            value: aaa
      serviceAccountName: agent-runner
