apiVersion: apps/v1beta2
kind: Deployment
metadata:
  name: proxy-agent-ccc
  labels:
    app: proxy-agent-ccc
spec:
  selector:
    matchLabels:
      app: proxy-agent-ccc
  template:
    metadata:
      labels:
        app: proxy-agent-ccc
    spec:
      containers:
      - name: proxy-agent
        image: gcr.io/mam-nooage/agent:sed
        imagePullPolicy: Always
        env:
          # Must be the same as in agent/service.yaml
          - name: JUPYTERLAB_SERVICE_NAME
            value: jupyterlab-ccc
          # Used to create the unique configmap when deploying the agent.
          - name: JUPYTERLAB_ID
            value: ccc
      serviceAccountName: agent-runner
