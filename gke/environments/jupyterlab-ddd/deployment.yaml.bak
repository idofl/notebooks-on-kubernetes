apiVersion: apps/v1beta2
kind: Deployment
metadata:
  name: jupyterlab-ddd
  labels:
    app: jupyterlab-ddd
spec:
  selector:
    matchLabels:
      app: jupyterlab-ddd
  template:
    metadata:
      labels:
        app: jupyterlab-ddd
    spec:
      containers:
      - image: gcr.io/deeplearning-platform-release/base-cpu:latest
        imagePullPolicy: Always
        name: jupyterlab-ddd
        ports:
        - containerPort: 8080
      serviceAccountName: default
